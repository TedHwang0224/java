
public class ifelse_1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		boolean android = true;
		boolean ios = false;
		// 만약에 android가 true라면
		if (true);
		{
			System.out.println("안그로이드 입니다.");
		}
		// 그렇지 않다면
		else (false);
		{
			// ios 입니다. 출력.
			System.out.println("ios 입니다.");
		}
	}

}
