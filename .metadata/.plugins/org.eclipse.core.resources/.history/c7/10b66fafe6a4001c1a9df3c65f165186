
public class question {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		// Page 2
		System.out.println("Hello Java!");
		
		// Page 7
		boolean a = true;
		byte b = 10;
		short c = 20;
		int d =30;
		long e = 40L;
		float f = 12.5f;
		double g = 34.56;
		char h = 'k';
		String i = "hello java";
		System.out.println("boolean :" +a);
		System.out.println("byte :" +b);
		System.out.println("short :" +c);
		System.out.println("int :" +d);
		System.out.println("long :" +e);
		System.out.println("float :" +f);
		System.out.println("double :" +g);
		System.out.println("char :" +h);
		System.out.println("String :" +i);
		
		
		// Page 8
		byte aVar = 100;
		short bVar = 30000;
		int cVar = 50000;
		long dVar = 1000000;
		System.out.println("byte 타입 :" +aVar);
		System.out.println("short 타입 :" +bVar);
		System.out.println("int 타입 :" +cVar);
		System.out.println("long 타입 :" +dVar);
		
		
		// Page 9
		float eVar;
		double fVar;
		
		// eVal=10.2;
		eVar=20.1f;
		fVar=20.1;
		System.out.println("float 타입 :" +eVar);
		System.out.println("double 타입 :" +fVar);
		
		
		// Page 10
		char gVar='A';
		System.out.println("aVar : " + gVar);
		System.out.println("aVar : " + (int)gVar);
	}

}
